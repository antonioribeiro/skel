{
    "name": ":vendor_name/:package_name",
    "description": ":package_description",
    "keywords": [
        ":vendor_name",
        ":package_name"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email",
            "homepage": ":author_website",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
        "illuminate/support": "~4.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            ":VendorName\\:PackageName\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":VendorName\\:PackageName\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
